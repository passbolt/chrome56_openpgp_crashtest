{
  "manifest_version": 2,
  "name": "Openpgpjs Crashtest Extension",
  "version": "1.3.2",
  "description": "A minimal extension to crash test openpgpjs",
  "icons": {
    "128": "skin/icon-128.png"
  },
  "background": {
    "page": "index.html"
  },
  "browser_action": {
    "default_icon": "skin/icon-19.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "background",
    "*://*/*"
  ],
  "web_accessible_resources": [
    "data/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
